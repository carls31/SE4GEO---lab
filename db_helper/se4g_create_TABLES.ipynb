    "def table_exists(table_name, conn):\n",
    "    cur = conn.cursor()\n",
    "    cur.execute(f\"SELECT EXISTS (SELECT 1 FROM information_schema.tables WHERE LOWER(table_name) = LOWER('{table_name}'))\")\n",
    "    exists = cur.fetchone()[0]\n",
    "    cur.close()\n",
    "    return exists"
   ]
